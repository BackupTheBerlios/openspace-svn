<?xml version="1.0"?>
<OpenspaceConfig>
	<version>@PACKAGE_VERSION@</version>
	<path>@prefix@/share/@PACKAGE@/</path>
	<icons_theme>default</icons_theme>
	<first_run>true</first_run>
	<position>center</position>
	<icon_size>64</icon_size>
	<show_hidden_files>true</show_hidden_files>
	<mime_magic>false</mime_magic>
	  <fonts>
	         <normalfont>arial,90,,,normal,iso8859-1,0</normalfont>
    		 <captionfont1>arial,90,bold,,normal,iso8859-1,0</captionfont1>
   		 <captionfont2>times,80,,,normal,iso8859-1,0</captionfont2>
   		 <captionfont3>arial,90,,,normal,iso8859-1,0</captionfont3>
 	 </fonts>
	
	<colors>
		  <main>#adaf9c</main>
  		  <base>#d6d7d6</base>
   		  <border>Black</border>
   		  <fore>Black</fore>
   		  <hilite>Gray100</hilite>
   		  <shadow>#8d8d8d</shadow>
   		  <selfore>Gray100</selfore>
  		  <selback>Navy</selback>
  		  <tipfore>Black</tipfore>
  		  <tipback>#ffffe1</tipback>
  		  <selmenutext>Gray100</selmenutext>
  		  <selmenuback>Navy</selmenuback>
		
	</colors>
	<speed_delay>
		  <typing_speed>1000</typing_speed>
		  <typing_speed>1000</typing_speed>
   		  <click_speed>400</click_speed>
  		  <scroll_speed>80</scroll_speed>
  		  <scroll_delay>600</scroll_delay>
 		  <blink_speed>500</blink_speed>
  		  <anim_speed>10</anim_speed>
  		  <menu_pause>400</menu_pause>
  		  <tooltip_pause>800</tooltip_pause>
 		  <tooltip_time>3000</tooltip_time>
 		  <drag_delta>6</drag_delta>
 		  <wheel_lines>1</wheel_lines>
	</speed_delay>
	
 	<mainwindow>
		<width>704</width>
		<height>450</height>
	</mainwindow>
  
  	<defaultdir>
		<dir>{homedir}</dir>
		<type>local</type>
	</defaultdir>
	
  	<leftdir>
		<dir>{homedir}</dir>
		<type>local</type>
	</leftdir>
  
  	<rightdir>
		<dir>{homedir}</dir>
		<type>local</type>
	</rightdir>
	<dirs>
		<path>
			<dir/>
			<type/>
		</path>
	</dirs>
	
	<show_commands>false</show_commands>
  	<panels>single</panels>
  	
  	<commands>
	
	
		<run_shell>
			<exec>xterm  -bg black -fg white</exec>
			<icon>run_shell</icon>
			<text>terminal</text>
			<key>F3</key>
		</run_shell>	
		
		<nedit>
			<exec>nedit</exec>
		</nedit>
		
	
		
		<edit_image>
			<exec>gimp-2.0 {f}</exec>
		</edit_image>
		
		<view_tar>
			<exec>tar tf {f}</exec>
			<options>capture</options>
		</view_tar>
		<extract_tar>
			<exec>tar xf {f}</exec>
			<options>rescan</options>
		</extract_tar>
		
		
		<extract_tar_bz2>
			<exec>tar xjf {f}</exec>
			<options>rescan</options>
		</extract_tar_bz2>
		
		<extract_tar_gz>
			<exec>tar xzf {f}</exec>
			<options>rescan</options>
		</extract_tar_gz>
		<unrar>
			<exec>unrar x {f}</exec>
			<options>rescan</options>
		</unrar>
		<unzip>
			<exec>unzip {f}</exec>
			<options>rescan</options>
		</unzip>
		<copy>
			<type>INTERNAL</type>
			<key>F5</key>
		</copy>
		<move>
			<type>INTERNAL</type>
			<key>F6</key>
		</move>
		<select_all>
			<type>INTERNAL</type>
			<key>a</key>
			<key_mask>control</key_mask>
		</select_all>
		<refresh>
			<type>INTERNAL</type>
			<icon>reload</icon>
			<key>r</key>
			<key_mask>control</key_mask>
		</refresh>
		
		<home>
			<type>INTERNAL</type>
			<icon>home</icon>
		</home>
		
		<dirup>
			<type>INTERNAL</type>
			<icon>dirup</icon>
		</dirup>
		
		<change_view_mini_icons>
			<type>INTERNAL</type>
			<icon>smallicons</icon>
			<text>small icons</text>
		</change_view_mini_icons>
		
		<change_view_big_icons>
			<type>INTERNAL</type>
			<icon>bigicons</icon>
			<text>big icons</text>
		</change_view_big_icons>
		
		<change_view_detailed>
			<type>INTERNAL</type>
			<icon>details</icon>
			<text>details</text>
		</change_view_detailed>
		
		<show_hide_hidden_files>
			<type>INTERNAL</type>
			<text>hidden files</text>
		</show_hide_hidden_files>
		
		<clipboard_copy>
			<type>INTERNAL</type>
			<icon>copy</icon>
			<key>c</key>
			<key_mask>control</key_mask>
		</clipboard_copy>
		<clipboard_paste>
			<type>INTERNAL</type>
			<icon>paste</icon>
			<key>v</key>
			<key_mask>control</key_mask>
		</clipboard_paste>
		<clipboard_cut>
			<type>INTERNAL</type>
			<icon>cut</icon>
			<key>x</key>
			<key_mask>control</key_mask>
		</clipboard_cut>
		
		<switch_on_or_two_panel_mode>
			<type>INTERNAL</type>
			<icon>min</icon>
		</switch_on_or_two_panel_mode>
		
		<get_dir>
			<type>INTERNAL</type>
			<text>get</text>
			<icon>getdir</icon>
		</get_dir>
		
		<goto_dir>
			<type>INTERNAL</type>
			<text>go</text>
			<icon>goto</icon>
		</goto_dir>
		
		<filter_files>
			<type>INTERNAL</type>
			<icon>pattern</icon>
		</filter_files>
		
		
		<OSMkdir>
			<type>PLUGIN</type>
			<text>mkdir</text>
		</OSMkdir>
		<OSAccess>
			<type>PLUGIN</type>
			<text>access</text>
		</OSAccess>
		<remove>
			<type>INTERNAL</type>
			<key>Delete</key>
		</remove>
		<OSRename>
			<type>PLUGIN</type>
			<text>rename</text>
		</OSRename>
		<OSClone>
			<type>PLUGIN</type>
			<text>clone</text>
		</OSClone>
		<OSSymlink>
			<type>PLUGIN</type>
			<text>symlink</text>
		</OSSymlink>
		<OSCompress>
			<type>PLUGIN</type>
			<text>compress</text>
		</OSCompress>
		<OSEditSymlink>
			<type>PLUGIN</type>
			<text>edit symlink</text>
		</OSEditSymlink>
		<other>
			<type>INTERNAL</type>
			<icon>other</icon>
		</other>
		<special>
			<type>INTERNAL</type>
		</special>
		<totalsize>
			<type>INTERNAL</type>
		</totalsize>
		<new_file>
			<type>INTERNAL</type>
			<text>new file</text>
		</new_file>
		<umount>
			<exec>umount {f}</exec>
		</umount>
		<vfs_archive>
			<type>VFS</type>
			<exec>archive</exec>
		</vfs_archive>
		
		
	</commands>
  
  	
  	<file_types>
		
		
		<image>
			<icon>mime-image</icon>
			<types>
				<png>
						<commands>
							<command>edit_image</command>
							<command>view_image</command>
						</commands>
					
				</png>
				<svgPxml>
					<icon>mime-image-svg+xml</icon>
				</svgPxml>
				<x-xcf>
					<icon>mime-image-x-xcf</icon>
				</x-xcf>
			</types>	
		</image>
		
		<video>
			<icon>mime-video</icon>
			<types>
				<quicktime>
					<icon>mime-video-quicktime</icon>
				</quicktime>
			</types>
		</video>
		
		<audio>
			<icon>mime-audio</icon>
			<types>
				<x-pn-realaudio>
					<icon>mime-audio-x-pn-realaudio</icon>
				</x-pn-realaudio>
				<x-wav>
					<icon>mime-audio-x-wav</icon>
				</x-wav>
			</types>
		</audio>
		
		<text>
			<types>
				<html>
					<icon>mime-text-html</icon>
				</html>
				<x-cPPsrc>
					<icon>mime-text-x-c++src</icon>
				</x-cPPsrc>
				<x-chdr>
					<icon>mime-text-x-c++hdr</icon>
				</x-chdr>
				<x-cPPhdr>
					<icon>mime-text-x-c++hdr</icon>
				</x-cPPhdr>
				<x-java>
					<icon>mime-text-x-java</icon>
				</x-java>
				<x-patch>
					<icon>mime-text-x-patch</icon>
				</x-patch>
			</types>
			<icon>mime-text</icon>
		</text>
		
		<application>
			<types>
				<pdf>
					<icon>mime-application-pdf</icon>	
					
				</pdf>
				
							
				<vnd.oasis.opendocument.text>
					<icon>mime-application-vnd.oasis.opendocument.text</icon>
				</vnd.oasis.opendocument.text>
				
				<vnd.sun.xml.writer>
					<icon>mime-application-vnd.oasis.opendocument.text</icon>	
				</vnd.sun.xml.writer>
				
				<msword>
					<icon>mime-application-vnd.oasis.opendocument.text</icon>	
				</msword>
				
				<vnd.oasis.opendocument.presentation>
					<icon>mime-application-vnd.oasis.opendocument.presentation</icon>
				</vnd.oasis.opendocument.presentation>
				
				<vnd.sun.xml.impress>
					<icon>mime-application-vnd.oasis.opendocument.presentation</icon>
				</vnd.sun.xml.impress>
				
				<vnd.ms-powerpoint>
					<icon>mime-application-vnd.oasis.opendocument.presentation</icon>
				</vnd.ms-powerpoint>
				
				<vnd.oasis.opendocument.spreadsheet>
					<icon>mime-application-vnd.oasis.opendocument.spreadsheet</icon>
				</vnd.oasis.opendocument.spreadsheet>				
				
				<vnd.sun.xml.calc>
					<icon>mime-application-vnd.oasis.opendocument.spreadsheet</icon>	
				</vnd.sun.xml.calc>
				
				<vnd.ms-excel>
					<icon>mime-application-vnd.oasis.opendocument.spreadsheet</icon>					
				</vnd.ms-excel>
								
				<vnd.oasis.opendocument.graphics>
					<icon>mime-application-vnd.oasis.opendocument.graphics</icon>
				</vnd.oasis.opendocument.graphics>
				<vnd.sun.xml.draw>
					<icon>mime-application-vnd.oasis.opendocument.graphics</icon>
				</vnd.sun.xml.draw>
				
				
				
				<x-sharedlib>
					<icon>mime-application-x-sharedlib</icon>	
				</x-sharedlib>
				<x-shellscript>
					<icon>mime-application-x-shellscript</icon>
				</x-shellscript>
				<x-bzip-compressed-tar>
					<default>vfs_archive</default>
					<icon>mime-application-x-compressed-tar</icon>
					<commands>
							<command>extract_tar_bz2</command>
					</commands>
				</x-bzip-compressed-tar>
				<x-compressed-tar>
					<default>vfs_archive</default>
					<icon>mime-application-x-compressed-tar</icon>
					<commands>
							<command>extract_tar_gz</command>
					</commands>
				</x-compressed-tar>
				<x-rar>
					<icon>mime-application-x-compressed-tar</icon>
					<commands>
							<command>unrar</command>
					</commands>
				</x-rar>
				<zip>
					<icon>mime-application-x-compressed-tar</icon>
					<commands>
							<command>unzip</command>
					</commands>
				</zip>
				<x-cd-image>
					<icon>mime-application-x-cd-image</icon>
				</x-cd-image>
				<postscript>
					<icon>mime-application-postscript</icon>
				</postscript>
				
				<x-perl>
					<icon>mime-application-x-perl</icon>
				</x-perl>
				<x-object>
					<icon>mime-application-x-object</icon>
				</x-object>
				<x-python>
					<icon>mime-application-x-python</icon>
				</x-python>
				<x-php>
					<icon>mime-application-x-php</icon>
				</x-php>
				<x-tex>
					<icon>mime-application-x-tex</icon>
				</x-tex>
				
				<x-deb>
					<icon>mime-application-x-rpm</icon>
				</x-deb>
				<x-rpm>
					<icon>mime-application-x-rpm</icon>
				</x-rpm>
				
				
				
							
			</types>	
		</application>	
		<COMMON>
		<types>
		<dir>
			<icon>directory</icon>
			<color>#000064</color>
		
			<commands>
					<command>umount</command>
			</commands>
		</dir>
		<executable>
			<icon>executable</icon>
			<color>#006464</color>
		</executable>
		<all>
			<icon>unknown</icon>
			<default>open_text</default>
		</all>
		<symlink>
			<icon>symlink</icon>
		</symlink>
		</types>
		</COMMON>
		
	</file_types>
	
	
	<toolbars>
		<toolbar1>
			<command>home</command>
			<command>dirup</command>
			<command>refresh</command>
			<command>SEPARATOR</command>
			<command>change_view_big_icons</command>
			<command>change_view_mini_icons</command>			
			<command>change_view_detailed</command>
			<command>SEPARATOR</command>
			<command>clipboard_copy</command>
			<command>clipboard_cut</command>
			<command>clipboard_paste</command>
			<command>SEPARATOR</command>
			<command>switch_on_or_two_panel_mode</command>
			<command>SEPARATOR</command>
			<command>get_dir</command>
			<command>goto_dir</command>
			<command>filter_files</command>
		</toolbar1>
		<toolbar2>
			<command>run_shell</command>
			<command>other</command>
		</toolbar2>	
	</toolbars>		
	
	
	<shutter>
		<common>
			<command>copy</command>
			<command>move</command>
			<command>OSMkdir</command>
			<command>OSAccess</command>
			<command>remove</command>
			<command>OSRename</command>
			<command>OSClone</command>
			<command>OSSymlink</command>
			<command>OSEditSymlink</command>
			<command>special</command>
			<command>totalsize</command>
			<command>new_file</command>
		</common>
		<compress>
			<command>OSCompress</command>
		</compress>
		
		<panel>
			<command>other</command>
			<command>show_hide_hidden_files</command>
		</panel>
		
		<programs>
			<command>run_shell</command>
			<command>nedit</command>
		</programs>
	</shutter>
	
	<filelist>
		<local>
			<headers>
				<header>size</header>
				<header>owner</header>
				<header>group</header>
				<header>mode</header>
				<header>modified</header>
				
				
			</headers>
			<style>big icons</style>
			<properties>
				<name>
					<width>180</width>					
				</name>
				<size>
					<width>80</width>
					<display>MB</display>
					<type>size</type>
				</size>
				<owner>
					<width>80</width>
				</owner>
				<group>
					<width>80</width>
				</group>
				<mode>
					<width>80</width>
				</mode>
				<accessed>
					<width>100</width>
					<type>date</type>
				</accessed>
				<modified>
					<width>100</width>
					<type>date</type>
				</modified>
			</properties>
			<thumbs>
				<size>100000</size>
			</thumbs>
		</local>
		
		<ftp>
			<type>network</type>
			<style>big icons</style>
			<headers>
				<header>size</header>
				<header>owner</header>
				<header>group</header>
				<header>mode</header>
	
			</headers>
			<properties>
				<name>
					<width>400</width>					
				</name>
				<size>
					<width>80</width>
					<display>MB</display>
					<type>size</type>
				</size>
				<owner>
					<width>80</width>
				</owner>
				<group>
					<width>80</width>
				</group>
				<mode>
					<width>80</width>
				</mode>
			</properties>	
		</ftp>
		<search>
			<type>search</type>
			<style>small icons</style>
			<properties>
				<name>
					<width>400</width>					
				</name>
			</properties>	
		</search>
		<archive>
			<type>vfs</type>
			<style>small icons</style>
			<properties>
				<name>
					<width>400</width>					
				</name>
			</properties>	
		</archive>
		
		
		
	</filelist>
	
	
	
	
	
	
</OpenspaceConfig>
