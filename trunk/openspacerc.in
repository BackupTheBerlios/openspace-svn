<?xml version="1.0"?>
<OpenspaceConfig>
	<version>0.0.1</version>
	<path>@prefix@/share/@PACKAGE@/</path>
	
 	<mainwindow>
		<width>868</width>
		<height>600</height>
	</mainwindow>
  
  	<defaultdir>
		<dir>/</dir>
		<type>local</type>
	</defaultdir>
	
  	<leftdir>
		<dir>/</dir>
		<type>local</type>
	</leftdir>
  
  	<rightdir>
		<dir>/</dir>
		<type>local</type>
	</rightdir>
	<dirs>
		<path>
			<dir/>
			<type/>
		</path>
	</dirs>
	
	<show_commands>true</show_commands>
  
  	
  	<commands>
	
	
		<run_shell>
			<exec>aterm -transparent -foreground white -bg black -cr red -sl 500  -trsb -shading 80</exec>
		</run_shell>	
		
		<nedit>
			<exec>nedit</exec>
		</nedit>
		
		<view_txt>
			<exec>nedit {f}</exec>
		</view_txt>		
		<view_pdf>
			<exec>xpdf {f}</exec>
		</view_pdf>
		<view_doc>
			<exec>oowriter {f}</exec>
		</view_doc>
		<view_calc>
			<exec>oocalc {f}</exec>
		</view_calc>
		
		<edit_image>
			<exec>qiv {f}</exec>
		</edit_image>
		
		<view_image>
			<exec>qiv {f}</exec>
		</view_image>
		
		<view_tar>
			<exec>tar tf {f}</exec>
			<options>capture</options>
		</view_tar>
		<extract_tar>
			<exec>tar xf {f}</exec>
			<options>rescan</options>
		</extract_tar>
		
		
		<extract_tar_bz2>
			<exec>tar xjf {f}</exec>
			<options>rescan</options>
		</extract_tar_bz2>
		
		<extract_tar_gz>
			<exec>tar xzf {f}</exec>
			<options>rescan</options>
		</extract_tar_gz>
		<unrar>
			<exec>unrar x {f}</exec>
			<options>rescan</options>
		</unrar>
		<unzip>
			<exec>unzip {f}</exec>
			<options>rescan</options>
		</unzip>
		
		<play_video>
			<exec>mplayer -vo xv {f}</exec>
		</play_video>
		<play_music>
			<exec>xmms -e {f}</exec>
		</play_music>
		
		<copy>
			<exec>INTERNAL</exec>
		</copy>
		<move>
			<exec>INTERNAL</exec>
		</move>
		<mkdir>
			<exec>PLUGIN</exec>
		</mkdir>
		<access>
			<exec>PLUGIN</exec>
		</access>
		<remove>
			<exec>INTERNAL</exec>
		</remove>
		<rename>
			<exec>PLUGIN</exec>
		</rename>
		<other>
			<exec>INTERNAL</exec>
		</other>
		<ls>
			<exec>ls</exec>
			<options>capture</options>
		</ls>
		<special>
			<exec>INTERNAL</exec>
		</special>
		<totalsize>
			<exec>INTERNAL</exec>
		</totalsize>
		<new_file>
			<exec>INTERNAL</exec>
		</new_file>
		<umount>
			<exec>umount {f}</exec>
		</umount>
		<show_thumbs>
			<exec>gqview {f}</exec>
		</show_thumbs>
		
	</commands>
  
  
  	<file_types>
		<txt>
			<default>view_txt</default>			
			<icon>txt.gif</icon>
		</txt>
		
		<tar.bz2>
			<default>extract_tar_bz2</default>
			<icon>compressed.gif</icon>
		</tar.bz2>
		<tar.gz>
			<default>extract_tar_gz</default>
			<icon>compressed.gif</icon>
		</tar.gz>
		<rar>
			<default>unrar</default>
			<icon>compressed.gif</icon>
		</rar>
		<zip>
			<default>unzip</default>
			<icon>compressed.gif</icon>
		</zip>

		<sxw>
			<default>view_doc</default>			
			<icon>word.gif</icon>
		</sxw>
		<sxc>
			<default>view_doc</default>			
			<icon>calc.gif</icon>
		</sxc>
		<xls>
			<default>view_doc</default>			
			<icon>calc.gif</icon>
		</xls>
		<so>
			<icon>library.gif</icon>
		</so>
		<doc>
			<default>view_doc</default>			
			<icon>word.gif</icon>
			<color>100 0 100</color>
		</doc>
		<pdf>
			<default>view_pdf</default>
			<icon>doc.gif</icon>
		</pdf>	
		<sh>
			<icon>script.gif</icon>
		</sh>	
		<mp3>
			<default>play_music</default>
			<icon>music.gif</icon>
		</mp3>			
		<pdf>
			<default>view_pdf</default>
			<icon>doc.gif</icon>
		</pdf>	
			
		<jpg>
			<default>view_image</default>
				<commands>
					<command>edit_image</command>
					<command>view_image</command>
				</commands>
			<icon>image.gif</icon>	
					
		</jpg>
		<png>
			<default>view_image</default>
				<commands>
					<command>edit_image</command>
					<command>view_image</command>
				</commands>
			<icon>image.gif</icon>	
					
		</png>
		<bmp>
			<default>view_image</default>
				<commands>
					<command>edit_image</command>
					<command>view_image</command>
				</commands>
			<icon>image.gif</icon>	
					
		</bmp>
		<gif>
			<default>view_image</default>
				<commands>
					<command>edit_image</command>
					<command>view_image</command>
				</commands>
			<icon>image.gif</icon>	
					
		</gif>
		<avi>
			<default>play_video</default>
			<icon>movie.gif</icon>						
		</avi>
		<mpg>
			<default>play_video</default>
			<icon>movie.gif</icon>						
		</mpg>
		<mpeg>
			<default>play_video</default>
			<icon>.gif</icon>						
		</mpeg>
		
		
	</file_types>
	
	<file_types_special>
		<dir>
			<color>0 0 100</color>
			<backcolor>145 134 201</backcolor>
			<commands>
					<command>umount</command>
					<command>show_thumbs</command>
			</commands>
		</dir>
		<executable>
			<color>0 100 100</color>
		</executable>
		<all>
			<default>view_txt</default>
		</all>	
	</file_types_special>
	
	<shutter>
		<common>
			<command>copy</command>
			<command>move</command>
			<command>mkdir</command>
			<command>access</command>
			<command>remove</command>
			<command>rename</command>
			<command>special</command>
			<command>totalsize</command>
			<command>new_file</command>
			<command>ls</command>
		</common>
		
		<panel>
			<command>other</command>
		</panel>
		
		<programs>
			<command>run_shell</command>
			<command>nedit</command>
		</programs>
	</shutter>
	
	<filelist>
		<local>
			<headers>
				<header>size</header>
				<header>owner</header>
				<header>group</header>
				<header>accessed</header>
				<header>mode</header>
				
				
			</headers>
			<style>big icons</style>
			<properties>
				<name>
					<width>180</width>
				</name>
				<size>
					<width>80</width>
					<display>MB</display>
				</size>
				<owner>
					<width>80</width>
				</owner>
				<mode>
					<width>80</width>
				</mode>
				<accessed>
					<width>200</width>
				</accessed>
			</properties>
		</local>
		<scp>
			<headers>
				<header>size</header>
				<header>owner</header>
				<header>group</header>
				<header>accessed</header>
			</headers>
			<properties>
				<name>
					<width>180</width>
				</name>
				<size>
					<width>80</width>
					<display>MB</display>
				</size>
				
				<owner>
					<width>80</width>
				</owner>
				<accessed>
					<width>200</width>
				</accessed>
			</properties>
		</scp>
		
		
	</filelist>
	
	<plugins>
		<cmddialog>
			<mkdir>mkdir</mkdir>
			<access>access</access>
			<rename>rename</rename>
		</cmddialog>
	</plugins>
	
	
	
	
</OpenspaceConfig>
